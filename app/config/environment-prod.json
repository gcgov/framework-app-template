{
  "type": "prod",
  "serverName": "app.prod",
  "rootUrl": "{prod_app_root_url}",
  "basePath": "{prod_app_base_path}",
  "phpPath": "{prod_app_php_path}\\php-cgi.exe -c {prod_app_absolute_path}\\srv\\app.prod\\php.ini",
  "mongoDatabases": [
	{
	  "default": true,
	  "database": "",
	  "uri": "",
	  "audit": false,
	  "include_meta": false,
